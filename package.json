    "@tomfreudenberg/next-auth-mock": "^0.5.6",
    "axios": "^0.26.1",
    "chart.js": "^2.9.4",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.4",
    "faker": "5.5.3",
    "framer-motion": "^6.3.16",
    "glob-parent": "5.1.2",
