{
  "name": "seasoning",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.13",
    "yarn": "please use npm",
    "pnpm": "please use npm",
    "pn": "please use npm"
  },
  "scripts": {
    "dev": "NEXT_PUBLIC_NAME=$npm_package_name NEXT_PUBLIC_VERSION=$npm_package_version next dev",
    "build": "NEXT_PUBLIC_NAME=$npm_package_name NEXT_PUBLIC_VERSION=$npm_package_version test & lint & next build ",
    "start": "next start",
    "lint": "npm-run-all -p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint . --ext \".js,.ts,.jsx,.tsx\"",
    "lint:prettier": "prettier --check .",
    "lint:types": "tsc --pretty --noEmit",
    "lint:style": "stylelint '**/*.{css,scss,sass}'",
    "fix": "run-s fix:prettier fix:eslint fix:style",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix:style": "stylelint --fix '**/*.{css,scss,sass}'",
    "test": "jest --",
    "test:watch": "jest --watch",
    "swagger": "openapi --input http://localhost:8888/swagger/ --output ./lib/swagger/ --client axios --name SwaggerClient",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "serve-storybook": "serve storybook-static"
  },
  "dependencies": {
    "@date-io/dayjs": "^2.14.0",
